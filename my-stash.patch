+  warn: (body, attributes = {}) =>
+    emitLog(SeverityNumber.WARN, 'WARN', body, attributes),
+  error: (body, attributes = {}) =>
+    emitLog(SeverityNumber.ERROR, 'ERROR', body, attributes),
+};
+
+// On the server, keep pino with the OTEL transport.
