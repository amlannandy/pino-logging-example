+    case 30:
+      return { num: SeverityNumber.INFO, text: 'INFO' };
+    case 40:
+      return { num: SeverityNumber.WARN, text: 'WARN' };
+    case 50:
+      return { num: SeverityNumber.ERROR, text: 'ERROR' };
+    case 60:
+      return { num: SeverityNumber.FATAL, text: 'FATAL' };
+    default:
+      return { num: SeverityNumber.INFO, text: 'INFO' };
