# SigNoz OTLP HTTP endpoint or region (server-side)
SIGNOZ_ENDPOINT=
# or set region to build cloud endpoint: https://ingest.<region>.signoz.cloud:443/v1/logs
SIGNOZ_REGION=

# SigNoz ingestion key (required for SigNoz Cloud)
SIGNOZ_INGESTION_KEY=

# Optional service metadata (server-side)
SIGNOZ_SERVICE_NAME=next-log-example
SIGNOZ_SERVICE_NAMESPACE=

# Client-side equivalents (must be prefixed NEXT_PUBLIC_ to be exposed to the browser)
NEXT_PUBLIC_SIGNOZ_ENDPOINT=
NEXT_PUBLIC_SIGNOZ_REGION=
NEXT_PUBLIC_SIGNOZ_INGESTION_KEY=
NEXT_PUBLIC_SIGNOZ_SERVICE_NAME=next-log-example
NEXT_PUBLIC_SIGNOZ_SERVICE_NAMESPACE=

# Optional: override log level for pino
LOG_LEVEL=debug

